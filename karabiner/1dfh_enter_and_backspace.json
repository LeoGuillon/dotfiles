{
  "description": "closer enter (on quote) and backspace (on opening bracket)",
  "manipulators": [
    {
      "type": "basic",
      "description": "quote -> enter",
      "from": {
        "key_code": "quote",
        "modifiers": {
          "optional": [
            "any"
          ]
        }
      },
      "to": [
        {
          "key_code": "return_or_enter"
        }
      ]
    },
    {
      "type": "basic",
      "description": "open_bracket -> delete_or_backspace",
      "from": {
        "key_code": "open_bracket",
        "modifiers": {
          "optional": [
            "any"
          ]
        }
      },
      "to": [
        {
          "key_code": "delete_or_backspace"
        }
      ]
    }
  ]
}
