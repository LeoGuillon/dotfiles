{
  "agreg-dev": {
    "body": [
      "\\subsection{${1:name}}\\label{dev:${2:label}}",
      "",
      "Leçons correspondantes : ${3:lessons}",
      "",
      "\\begin{${4:env}*}{}{}",
      "  ${5:question}",
      "\\end{${4:env}*}",
      "",
      "\\begin{proof}",
      "  ${6:proof}",
      "\\end{proof}"
    ],
    "prefix": "agreg-dev"
  },
  "agreg-lesson": {
    "body": [
      "\\addcontentsline{toc}{subsection}{${1:lesson}}",
      "\\subsection*{${1:lesson}}\\label{lesson:${2:lesson-number}}",
      "",
      "\\paragraph{Introduction}",
      "",
      "\\paragraph{Plan}",
      "",
      "\\paragraph{Développements}",
      "",
      "\\paragraph{Notes}",
      "",
      "\\paragraph{Sources}"
    ],
    "prefix": "agreg-lesson"
  }
}
