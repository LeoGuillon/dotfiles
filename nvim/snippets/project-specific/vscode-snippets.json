{
  "snippet + vscode-snippet": {
    "body": [
      "\"${1:name}\": {",
      "  \"body\": [",
      "    \"${2:content}\"",
      "  ],",
      "  \"prefix\": [",
      "    \"${3:prefix}\",",
      "  ],",
      "  \"description\": \"${4:description}\"",
      "},"
    ],
    "prefix": [
      "snippet",
      "vscode-snippet"
    ]
  },
  "snippets-file": {
    "body": [
      "{",
      "  \"path\": \"./${1:path}\",",
      "  \"language\": [",
      "    \"${2:language}\"",
      "  ]",
      "},"
    ],
    "prefix": "snippets-file"
  }
}
