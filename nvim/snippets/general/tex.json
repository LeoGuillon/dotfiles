{
  "abstract": {
    "body": [
      "\\begin{abstract}",
      "  $0",
      "\\end{abstract}"
    ],
    "prefix": "abstract"
  },
  "ac": {
    "body": "\\ac{$1}$0",
    "prefix": "ac"
  },
  "acro": {
    "body": "\\acro{${1:acronym}}{${2:full name}}",
    "prefix": "acro"
  },
  "acrodef": {
    "body": "\\acrodef{${1:acronym}}{${2:full name}}",
    "prefix": "acrodef"
  },
  "acronym": {
    "body": [
      "\\begin{acronym}",
      "  $0",
      "\\end{acronym}"
    ],
    "prefix": "acronym"
  },
  "bf + bold": {
    "body": "\\textbf{${1:${TM_SELECTED_TEXT}}}$0",
    "prefix": [
      "bf",
      "bold"
    ]
  },
  "block": {
    "body": [
      "\\begin{block}{$1}",
      "  $0",
      "\\end{block}"
    ],
    "prefix": "block"
  },
  "columns-2": {
    "body": [
      "\\begin{columns}",
      "  \\begin{column}{0.5\\textwidth}",
      "    ",
      "  \\end{column}",
      "  \\begin{column}{0.5\\textwidth}",
      "    ",
      "  \\end{column}",
      "\\end{columns}"
    ],
    "prefix": "columns-2"
  },
  "em": {
    "body": "\\emph{${1:${TM_SELECTED_TEXT}}}$0",
    "prefix": [
      "em",
      "emph"
    ]
  },
  "figure": {
    "body": [
      "\\begin{figure}",
      "  \\centering",
      "  \\includegraphics[width=$1\\textwidth]{$2}",
      "  \\caption{$3}",
      "  \\label{fig:$2}",
      "\\end{figure}",
      "$0"
    ],
    "prefix": "figure"
  },
  "figure:tikz": {
    "body": [
      "\\begin{figure}",
      "  \\centering",
      "  \\input{src/tikz/$1}",
      "  \\caption{$2}",
      "  \\label{fig:$1}",
      "\\end{figure}",
      "$0"
    ],
    "prefix": "figure:tikz"
  },
  "frame": {
    "body": [
      "\\begin{frame}{${1:frametitle}}",
      "  $0",
      "\\end{frame}"
    ],
    "prefix": "frame"
  },
  "include": {
    "body": "\\include{$1}",
    "prefix": "include"
  },
  "it + italic": {
    "body": "\\textit{${1:${TM_SELECTED_TEXT}}}$0",
    "prefix": [
      "it",
      "italic"
    ]
  },
  "newcommand": {
    "body": "\\newcommand\\\\$1[${2:parameters_number}]{${3:command}}",
    "prefix": "newcommand-parameters"
  },
  "newcommand-bis": {
    "body": "\\newcommand\\\\$1{${2:command}}",
    "prefix": "newcommand"
  },
  "node": {
    "body": "\\node ($1) [$2] {$3};",
    "prefix": "node"
  },
  "paragraph": {
    "body": [
      "\\paragraph{$1}\\label{par:$2}",
      "$0"
    ],
    "prefix": [
      "paragraph",
      "par"
    ]
  },
  "paragraph:ref": {
    "body": "Paragraph~\\ref{par:$1}$0",
    "prefix": "paragraph:ref"
  },
  "pause": {
    "body": [
      "\\vfill",
      "\\pause"
    ],
    "prefix": "pause"
  },
  "section": {
    "body": [
      "\\section{$1}\\label{sec:$2}",
      "",
      "$0"
    ],
    "prefix": "section"
  },
  "section*": {
    "body": "\\section*{$1}",
    "prefix": "section*"
  },
  "subsection": {
    "body": [
      "\\subsection{$1}\\label{sec:$2}",
      "",
      "$0"
    ],
    "prefix": "subsection"
  },
  "table-input": {
    "body": [
      "\\begin{table}",
      "  \\centering",
      "  \\input{src/tables/$1}",
      "  \\caption{$2}",
      "  \\label{tab:${1/(.+?)(tex)?/$1/}}",
      "\\end{table}",
      "$0"
    ],
    "prefix": "table"
  },
  "tt": {
    "body": "\\texttt{${1:${TM_SELECTED_TEXT}}}$0",
    "prefix": [
      "tt",
      "typewriter"
    ]
  },
  "usepackage": {
    "body": "\\usepackage{$1}",
    "prefix": [
      "usepackage",
      "\\usepackage",
      "package",
      "\\package"
    ]
  },
  "verbatim": {
    "body": [
      "\\begin{verbatim}",
      "  $0",
      "\\end{verbatim}"
    ],
    "prefix": "verbatim"
  }
}
