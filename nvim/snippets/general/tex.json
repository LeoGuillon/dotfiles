{
  "RequirePackage": {
    "body": "\\RequirePackage{${1:package}}",
    "prefix": "RequirePackage"
  },
  "WarningFilter": {
    "body": "\\WarningFilter{${1:package}}{${2:command}}",
    "prefix": "WarningFilter"
  },
  "abstract": {
    "body": [
      "\\begin{abstract}",
      "  $0",
      "\\end{abstract}"
    ],
    "prefix": "abstract"
  },
  "ac": {
    "body": "\\ac{$1}$0",
    "prefix": "ac"
  },
  "acro": {
    "body": "\\acro{${1:acronym}}{${2:full name}}",
    "prefix": "acro"
  },
  "acrodef": {
    "body": "\\acrodef{${1:acronym}}{${2:full name}}",
    "prefix": "acrodef"
  },
  "acronym": {
    "body": [
      "\\begin{acronym}",
      "  $0",
      "\\end{acronym}"
    ],
    "prefix": "acronym"
  },
  "bf + bold": {
    "body": "\\textbf{${1:${TM_SELECTED_TEXT}}}$0",
    "prefix": [
      "bf",
      "bold"
    ]
  },
  "block": {
    "body": [
      "\\begin{block}{$1}",
      "  $0",
      "\\end{block}"
    ],
    "prefix": "block"
  },
  "columns-2": {
    "body": [
      "\\begin{columns}",
      "  \\begin{column}{0.5\\textwidth}",
      "    ",
      "  \\end{column}",
      "  \\begin{column}{0.5\\textwidth}",
      "    ",
      "  \\end{column}",
      "\\end{columns}"
    ],
    "prefix": "columns-2"
  },
  "cref": {
    "body": "\\cref{$1}",
    "prefix": "cref"
  },
  "crefname": {
    "body": "\\crefname{${1:environment}}{${2:cref}}{${3:plural cref}}",
    "prefix": "crefname"
  },
  "declaremathoperator + mathoperator": {
    "body": "\\DeclareMathOperator{${1:operator}}{${2:writing}} % ${3:comment}",
    "prefix": [
      "declaremathoperator",
      "mathoperator"
    ]
  },
  "definecolor_RGB": {
    "body": "\\definecolor{${1:name}}{RGB}{${2:R},${3:G},${4:B}}",
    "prefix": "definecolor_RGB"
  },
  "em": {
    "body": "\\emph{${1:${TM_SELECTED_TEXT}}}$0",
    "prefix": [
      "em",
      "emph"
    ]
  },
  "figure": {
    "body": [
      "\\begin{figure}",
      "  \\centering",
      "  \\includegraphics[width=$1\\textwidth]{$2}",
      "  \\caption{$3}",
      "  \\label{fig:$2}",
      "\\end{figure}",
      "$0"
    ],
    "prefix": "figure"
  },
  "figure:tikz": {
    "body": [
      "\\begin{figure}",
      "  \\centering",
      "  \\input{src/tikz/$1}",
      "  \\caption{$2}",
      "  \\label{fig:$1}",
      "\\end{figure}",
      "$0"
    ],
    "prefix": "figure:tikz"
  },
  "frame": {
    "body": [
      "\\begin{frame}{${1:frametitle}}",
      "  $0",
      "\\end{frame}"
    ],
    "prefix": "frame"
  },
  "href": {
    "body": "\\href{${1:url}}{${2:displayed text}}",
    "prefix": "href"
  },
  "include": {
    "body": "\\include{$1}",
    "prefix": "include"
  },
  "it + italic": {
    "body": "\\textit{${1:${TM_SELECTED_TEXT}}} $0",
    "prefix": [
      "it",
      "italic"
    ]
  },
  "item": {
    "body": "\\item $0",
    "prefix": "item"
  },
  "itemize": {
    "body": [
      "\\begin{itemize}",
      "  \\item $0 ",
      "\\end{itemize}"
    ],
    "prefix": "itemize"
  },
  "latex": {
    "body": "\\LaTeX",
    "prefix": "latex"
  },
  "metadata": {
    "body": [
      "\\title{${1:title}}",
      "\\author{LÃ©o Guillon}",
      "\\date{}"
    ],
    "prefix": "metadata"
  },
  "newcommand": {
    "body": "\\newcommand\\\\$1[${2:parameters_number}]{${3:command}} % ${4:description}",
    "prefix": "newcommand-parameters"
  },
  "newcommand-bis": {
    "body": "\\newcommand\\\\$1{${2:command}} % ${3:description}",
    "prefix": "newcommand"
  },
  "newtheorem-qed": {
    "body": [
      "\\newtheorem{${1:environment}}{${2:name}}",
      "\\crefname{$1}{$2}{$2s}",
      "\\setEnvironmentQed{$1}{\\ensuremath{${3:qed}}}"
    ],
    "prefix": "newtheorem-qed"
  },
  "node": {
    "body": "\\node ($1) [$2] {$3};",
    "prefix": "node"
  },
  "notation-line": {
    "body": "\\texttt{${1:command}} & \\$\\\\${1}\\$ & ${2:meaning}\\\\\\\\",
    "prefix": "notation-line"
  },
  "paragraph": {
    "body": [
      "\\paragraph{$1}\\label{par:$2}",
      "$0"
    ],
    "prefix": [
      "paragraph",
      "par"
    ]
  },
  "paragraph:ref": {
    "body": "Paragraph~\\ref{par:$1}$0",
    "prefix": "paragraph:ref"
  },
  "pause": {
    "body": [
      "\\vfill",
      "\\pause"
    ],
    "prefix": "pause"
  },
  "providetheorem": {
    "body": [
      "\\newtheorem{${1:environment}}{${2:name}}",
      "\\crefname{${1:environment}}{${2:name}}{$2s}"
    ],
    "prefix": "newtheorem"
  },
  "renewcommand": {
    "body": "\\renewcommand${1:original command}{${2:new command}}",
    "prefix": "renewcommand"
  },
  "section": {
    "body": [
      "\\section{$1}\\label{sec:$2}",
      "",
      "$0"
    ],
    "prefix": "section"
  },
  "section*": {
    "body": "\\section*{$1}",
    "prefix": "section*"
  },
  "strong": {
    "body": "\\strong{$1}",
    "prefix": "strong"
  },
  "subsection": {
    "body": [
      "\\subsection{$1}\\label{sec:$2}",
      "",
      "$0"
    ],
    "prefix": "subsection"
  },
  "table-input": {
    "body": [
      "\\begin{table}",
      "  \\centering",
      "  \\input{src/tables/$1}",
      "  \\caption{$2}",
      "  \\label{tab:${1/(.+?)(tex)?/$1/}}",
      "\\end{table}",
      "$0"
    ],
    "prefix": "table"
  },
  "title + maketitle": {
    "body": "\\maketitle",
    "prefix": [
      "title",
      "maketitle"
    ]
  },
  "tt": {
    "body": "\\texttt{${1:${TM_SELECTED_TEXT}}} $0",
    "prefix": [
      "tt",
      "typewriter"
    ]
  },
  "usepackage": {
    "body": "\\usepackage{$1}",
    "prefix": [
      "usepackage",
      "\\usepackage",
      "package",
      "\\package"
    ]
  },
  "usepackage-parameters": {
    "body": "\\usepackage[${1:parameters}]{${2:package}}",
    "prefix": "usepackage-parameters"
  },
  "verbatim": {
    "body": [
      "\\begin{verbatim}",
      "  $0",
      "\\end{verbatim}"
    ],
    "prefix": "verbatim"
  }
}
