{
  "definition": {
    "body": [
      "\\begin{definition}\\label{def:${1:label}}",
      "  ${2:definition} \\defterm{${3:term}} ${4:definition}",
      "\\end{definition}"
    ],
    "prefix": "definition"
  },
  "env": {
    "body": [
      "\\begin{${1:env}}",
      "  $0",
      "\\end{${1:env}}"
    ],
    "prefix": [
      "begin",
      "env"
    ]
  },
  "lemma": {
    "body": [
      "\\begin{lemma}[${1:name}]\\label{lemma:${2:label}}",
      "  ${3:lemma}",
      "\\end{lemma}"
    ],
    "prefix": "lemma"
  },
  "theorem": {
    "body": [
      "\\begin{theorem}[${1:name}]\\label{thm:${2:label}}",
      "  ${3:theorem}",
      "\\end{theorem}"
    ],
    "prefix": "theorem"
  }
}
