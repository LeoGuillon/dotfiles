{
  "abstract": {
    "body": [
      "\\begin{abstract}",
      "  $0",
      "\\end{abstract}"
    ],
    "prefix": "abstract"
  },
  "block": {
    "body": [
      "\\begin{block}{$1}",
      "  $0",
      "\\end{block}"
    ],
    "prefix": "block"
  },
  "chapter": {
    "body": [
      "\\chapter{${1:title}}\\label{chap:${2:label}}",
      "",
      "$0"
    ],
    "prefix": "chapter"
  },
  "columns-2": {
    "body": [
      "\\begin{columns}",
      "  \\begin{column}{0.5\\textwidth}",
      "    ",
      "  \\end{column}",
      "  \\begin{column}{0.5\\textwidth}",
      "    ",
      "  \\end{column}",
      "\\end{columns}"
    ],
    "prefix": "columns-2"
  },
  "definition": {
    "body": [
      "\\begin{definition}{${1:title}}{${2:label}}",
      "  ${3:definition} \\defterm{${1/(.)(.*)/${1:/downcase}${2}/}} ${5:definition}",
      "\\end{definition}"
    ],
    "prefix": "definition"
  },
  "description": {
    "body": [
      "\\begin{description}",
      "  \\item[$1] $2 ",
      "\\end{description}"
    ],
    "prefix": "description"
  },
  "env": {
    "body": [
      "\\begin{${1:env}}",
      "  $0",
      "\\end{${1:env}}"
    ],
    "prefix": [
      "begin",
      "env"
    ]
  },
  "figure": {
    "body": [
      "\\begin{figure}",
      "  \\centering",
      "  \\includegraphics[width=$1\\textwidth]{$2}",
      "  \\caption{$3}",
      "  \\label{fig:$2}",
      "\\end{figure}",
      "$0"
    ],
    "prefix": "figure"
  },
  "figure:tikz": {
    "body": [
      "\\begin{figure}",
      "  \\centering",
      "  \\input{src/tikz/$1}",
      "  \\caption{$2}",
      "  \\label{fig:$1}",
      "\\end{figure}",
      "$0"
    ],
    "prefix": "figure:tikz"
  },
  "itemize": {
    "body": [
      "\\begin{itemize}",
      "  \\item $0 ",
      "\\end{itemize}"
    ],
    "prefix": "itemize"
  },
  "lemma": {
    "body": [
      "\\begin{lemma}[${1:name}]\\label{lemma:${2:label}}",
      "  ${3:lemma}",
      "\\end{lemma}"
    ],
    "prefix": "lemma"
  },
  "table-input": {
    "body": [
      "\\begin{table}",
      "  \\centering",
      "  \\input{src/tables/$1}",
      "  \\caption{$2}",
      "  \\label{tab:${1/(.+?)(tex)?/$1/}}",
      "\\end{table}",
      "$0"
    ],
    "prefix": "table"
  },
  "theorem": {
    "body": [
      "\\begin{theorem}{${1:name}}{${2:label}}",
      "  ${3:${TM_SELECTED_TEXT}}",
      "\\end{theorem}"
    ],
    "prefix": "theorem"
  },
  "verbatim": {
    "body": [
      "\\begin{verbatim}",
      "  $0",
      "\\end{verbatim}"
    ],
    "prefix": "verbatim"
  }
}
