{
  "BUG": {
    "body": "$LINE_COMMENT BUG: $1",
    "prefix": [
      "bug",
      "fix"
    ]
  },
  "HACK": {
    "body": "$LINE_COMMENT HACK: $1",
    "prefix": [
      "hack"
    ]
  },
  "NOTE": {
    "body": "$LINE_COMMENT NOTE: $1",
    "prefix": [
      "note"
    ]
  },
  "QUES": {
    "body": "$LINE_COMMENT QUES: $1",
    "prefix": [
      "ques"
    ]
  },
  "TODO": {
    "body": "$LINE_COMMENT TODO: $1",
    "prefix": [
      "todo"
    ]
  },
  "WARN": {
    "body": "$LINE_COMMENT WARN: $1",
    "prefix": [
      "warn"
    ]
  },
  "comment_h1": {
    "body": [
      "$LINE_COMMENT ────────────────────────────────────────────────────────────────────────────────",
      "$LINE_COMMENT ($1)",
      "$LINE_COMMENT ────────────────────────────────────────────────────────────────────────────────",
      "",
      "$0"
    ],
    "prefix": "comment_h1"
  },
  "comment_h2": {
    "body": [
      "$LINE_COMMENT ────────────────────────────────────────────────────────────────────────────────",
      "$LINE_COMMENT ($0)",
      "",
      "$1"
    ],
    "prefix": "comment_h2"
  },
  "comment_line": {
    "body": "$LINE_COMMENT ────────────────────────────────────────────────────────────────────────────────",
    "prefix": "comment_line"
  }
}
